{
  "name": "hellovsc",
	"displayName": "test-ext",
	"description": "print hello on a little side bar",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:hellovsc.captureCursor",
        "onCommand:hellovsc.actiavteCursors"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "hellovsc.captureCursor",
				"title": "Capture Cursor"
			},
			{
				"command": "hellovsc.actiavteCursors",
				"title": "Activate Cursors"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.54.0",
        "@types/glob": "^7.1.4",
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "eslint": "^8.1.0",
        "glob": "^7.1.7",
        "mocha": "^9.1.3",
        "typescript": "^4.4.4",
        "@vscode/test-electron": "^1.6.2"
    }

}
