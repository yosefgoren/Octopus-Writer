{
  "name": "OctopusWriter",
	"displayName": "Octopus-Writer",
	"description": "this extension is meant to provide new text editing tools to allow a faster workflow",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:octopus_writer.captureCursor",
        "onCommand:octopus_writer.actiavteCursors"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "octopus_writer.captureCursor",
				"title": "Capture Cursor"
			},
			{
				"command": "octopus_writer.actiavteCursors",
				"title": "Activate Cursors"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.54.0",
        "@types/glob": "^7.1.4",
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "eslint": "^8.1.0",
        "glob": "^7.1.7",
        "mocha": "^9.1.3",
        "typescript": "^4.4.4",
        "@vscode/test-electron": "^1.6.2"
    },
	"repository": "https://github.com/yosefgoren/Octopus-Writer.git"
}
